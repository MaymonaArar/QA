    "test = False\n",
    "def MagicNumber_function():\n",
    "    test = False\n",
    "    with open('Magic number code.txt') as f:\n",
    "        lines = f.readlines()\n",
    "        for line in lines:\n",
    "            for char in line:\n",
    "                if char.isdigit():\n",
    "                    test = True \n",
    "    if test :\n",
    "        print(\"code have a magic number :( \")\n",
    "    if test==False  :\n",
    "        print(\"code not have a magic number :) \")\n",
    "\n",
    "########################################################################################\n",
    "def attributes_function():\n",
    "    typecount = 0\n",
    "    dataType = []\n",
    "    data = []\n",
    "    funName = \" \"\n",
    "    count=0\n",
    "    with open('Unreachable code.txt','r') as f:\n",
    "        for line in f:\n",
    "            for word in line.split():\n",
    "                if  word=='int' or word=='string' or word=='float' or word=='char' or word=='bool' :\n",
    "                    dataType.append(word)\n",
    "                    typecount = typecount + 1 \n",
    "                    if typecount == 1 and word!=dataType[0] and word!='('   :\n",
    "                        funName = word \n",
    "    with open('Unreachable code.txt','r') as f:\n",
    "        for line in f:\n",
    "            for word in line.split():\n",
    "                if word == funName :\n",
    "                    count = count + 1 \n",
    "                    if count > 1 and word!='(' and word!=',' and word!=funName and word!=');' :\n",
    "                        data.append(word)\n",
    "    for i in data :\n",
    "        if type(data) == dataType[i+1]: \n",
    "            print(True)\n",
    "        else :\n",
    "            print(False)\n",
    "########################################################################################\n",
    "count= 0\n",
    "def ThreeParameters_function():\n",
    "    count= 0\n",
    "    with open('parameters in vaild.txt') as f:\n",
    "        lines = f.readlines()\n",
    "        for line in lines:\n",
    "            for char in line:\n",
    "                if char == \",\" :\n",
    "                    count +=1\n",
    "    if count <=2:\n",
    "        print(\"Less than three parameter for the method"\")\n",
    "    else:\n",
    "        print(\"More  than three parameter for the method"\")\n",
    "##########################################################################################\n",
    "numReturn = False \n",
    "newWord=0\n",
    "def Unreachable_function():\n",
    "    numReturn = False \n",
    "    newWord=0\n",
    "    with open('Unreachable code.txt','r') as f:\n",
    "        for line in f:\n",
    "            for word in line.split():\n",
    "                if word=='return':\n",
    "                    numReturn=True\n",
    "                if numReturn and word:\n",
    "                    newWord=newWord+1\n",
    "        if newWord > 2:\n",
    "            print(\"Unreachable code\") \n",
    "    \n",
    "        else :\n",
    "            print(\"Not unreachable code\")\n",
    "\n",
    "#############################################################################################\n",
    "print('-----------------------------------------------------------------------------------')\n",
    "print('Option 1 :- Magic number' )\n",
    "print('Option 2 :- Do the attributes of each parameter match')\n",
    "print('Option 3 :- Make sure there are no more than three parameters for the methods.')\n",
    "print('Option 4:- Unreachable code.')\n",
    "print('Option 5:- Exit')\n",
    "print('------------------------------------------------------------------------------------')\n",
    "option = int(input('Enter your choice: '))\n",
    "        \n",
    "#Check what choice was entered and act accordingly\n",
    "if option == 1:\n",
    "    MagicNumber_function()\n",
    "elif option == 2:\n",
    "    attributes_function()\n",
    "elif option == 3:\n",
    "    ThreeParameters_function()\n",
    "elif option == 4:\n",
    "    Unreachable_function()\n",
    "elif option == 5:\n",
    "    print('Thanks message before exiting')\n",
    "    exit()\n",
    "else:\n",
    "    print('Invalid option. Please enter a number between 1 and 4.')"
